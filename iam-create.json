{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": "ec2:CreateTags",
            "Resource": [
                "arn:aws:ec2:ap-northeast-1:*:instance/*",
                "arn:aws:ec2:ap-northeast-1:*:subnet/*",
                "arn:aws:ec2:ap-northeast-1:*:security-group/*",
                "arn:aws:ec2:ap-northeast-1:*:internet-gateway/*",
                "arn:aws:ec2:ap-northeast-1:*:vpc/*",
                "arn:aws:ec2:ap-northeast-1:*:volume/*",
                "arn:aws:ec2:ap-northeast-1:*:network-interface/*",
                "arn:aws:ec2:ap-northeast-1:*:route-table/*",
                "arn:aws:ec2:ap-northeast-1:*:network-acl/*"
            ]
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": "ec2:CreateRoute",
            "Resource": "arn:aws:ec2:ap-northeast-1:*:route-table/*"
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": "ec2:RunInstances",
            "Resource": [
                "arn:aws:ec2:ap-northeast-1:*:instance/*",
                "arn:aws:ec2:ap-northeast-1:*:subnet/*",
                "arn:aws:ec2:ap-northeast-1:*:security-group/*",
                "arn:aws:ec2:ap-northeast-1:*:volume/*",
                "arn:aws:ec2:ap-northeast-1:*:network-interface/*",
                "arn:aws:ec2:ap-northeast-1:*:key-pair/*",
                "arn:aws:ec2:ap-northeast-1::image/*"
            ]
        },
        {
            "Sid": "VisualEditor3",
            "Effect": "Allow",
            "Action": [
                "ec2:DescribeInstances",
                "ec2:CreateVpc",
                "ec2:AttachInternetGateway",
                "ec2:DescribeSecurityGroups",
                "ec2:AssociateRouteTable",
                "ec2:DescribeImages",
                "ec2:DescribeInternetGateways",
                "ec2:DescribeVpcs",
                "ec2:CreateInternetGateway",
                "sts:DecodeAuthorizationMessage",
                "ec2:CreateSubnet",
                "ec2:DescribeSubnets",
                "ec2:DescribeNetworkAcls",
                "ec2:DescribeRouteTables"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor4",
            "Effect": "Allow",
            "Action": [
                "ec2:RevokeSecurityGroupIngress",
                "ec2:AuthorizeSecurityGroupIngress"
            ],
            "Resource": "arn:aws:ec2:ap-northeast-1:*:security-group/*"
        }
    ]
}
